include "include/table.bzz"

t = {}
i = 0
while(i < 10) {
  t[i] = math.rng.uniform(1.0)
  i = i + 1
}

table.print(t)
log("")

table.sort(t, table.cmp_number)
table.print(t)
log("")

table.sort(t, function(a,b) { return not table.cmp_number(a,b) })
table.print(t)
log("")

table.shuffle(t)
table.print(t)
log("")
