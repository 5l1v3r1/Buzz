function members() {
  log("fb ", id, ": MEMBER")
}

function nonmembers() {
  log("fb ", id, ": NONMEMBER")
}

function init() {
  s1 = swarm.create(1)
  s1.select(id % 2 == 0)
  o1 = s1.others(2)
  s2 = swarm.create(3)
  s2.select(id < 5)
  o2 = s2.others(4)
}

function step() {
  s1.exec(members)
  o1.exec(nonmembers)
  s2.exec(members)
  o2.exec(nonmembers)
}
